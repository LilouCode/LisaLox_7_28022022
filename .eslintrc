{
    "root": true,
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "parser": "babel-eslint"
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": { "indent": ["warn", 2, { "SwitchCase": 1 }], "multiline-ternary": ["warn", "always-multiline"], "no-unused-vars": "off", "no-undef": "off", "no-inner-declarations": "off" }


